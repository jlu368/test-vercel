// This file is automatically generated by Keystone, do not modify it manually.
// Modify your Keystone config when you want to change this.

datasource sqlite {
  url               = env("DATABASE_URL")
  shadowDatabaseUrl = env("SHADOW_DATABASE_URL")
  provider          = "sqlite"
}

generator client {
  provider = "prisma-client-js"
}

model User {
  id        String    @id @default(cuid())
  name      String    @default("")
  email     String    @unique @default("")
  password  String
  createdAt DateTime? @default(now())
}

model Tag {
  id    String @id @default(cuid())
  name  String @default("")
  posts Post[] @relation("Post_tags")
}

model Service {
  id                            String            @id @default(cuid())
  title                         String            @default("")
  heroImage_id                  String?
  heroImage_filesize            Int?
  heroImage_width               Int?
  heroImage_height              Int?
  heroImage_extension           String?
  content                       String            @default("[{\"type\":\"paragraph\",\"children\":[{\"text\":\"\"}]}]")
  conditionsLabel               String            @default("")
  conditions                    Condition[]       @relation("Service_conditions")
  from_Condition_services       Condition[]       @relation("Condition_services")
  from_ServicesLanding_services ServicesLanding[] @relation("ServicesLanding_services")
  from_Home_services            Home[]            @relation("Home_services")
  from_Location_services        Location[]        @relation("Location_services")
}

model Condition {
  id                      String    @id @default(cuid())
  title                   String    @default("")
  heroImage_id            String?
  heroImage_filesize      Int?
  heroImage_width         Int?
  heroImage_height        Int?
  heroImage_extension     String?
  content                 String    @default("[{\"type\":\"paragraph\",\"children\":[{\"text\":\"\"}]}]")
  servicesLabel           String    @default("")
  services                Service[] @relation("Condition_services")
  from_Service_conditions Service[] @relation("Service_conditions")
}

model ServicesLanding {
  id       String    @id @default(cuid())
  title    String    @default("")
  services Service[] @relation("ServicesLanding_services")
}

model Post {
  id                  String    @id @default(cuid())
  title               String    @default("")
  heroImage_id        String?
  heroImage_filesize  Int?
  heroImage_width     Int?
  heroImage_height    Int?
  heroImage_extension String?
  abstract            String    @default("[{\"type\":\"paragraph\",\"children\":[{\"text\":\"\"}]}]")
  content             String    @default("[{\"type\":\"paragraph\",\"children\":[{\"text\":\"\"}]}]")
  date                String?
  author              Provider? @relation("Post_author", fields: [authorId], references: [id])
  authorId            String?   @map("author")
  tags                Tag[]     @relation("Post_tags")

  @@index([authorId])
}

model BlogLanding {
  id                  String  @id @default(cuid())
  title               String  @default("")
  heroImage_id        String?
  heroImage_filesize  Int?
  heroImage_width     Int?
  heroImage_height    Int?
  heroImage_extension String?
}

model Testimonial {
  id                     String @id @default(cuid())
  name                   String @default("")
  content                String @default("[{\"type\":\"paragraph\",\"children\":[{\"text\":\"\"}]}]")
  stars                  Int?
  from_Home_testimonials Home[] @relation("Home_testimonials")
}

model Home {
  id                  String        @id @default(cuid())
  heroHeadline        String        @default("")
  heroImage_id        String?
  heroImage_filesize  Int?
  heroImage_width     Int?
  heroImage_height    Int?
  heroImage_extension String?
  heroCtaLabel        String        @default("")
  heroCtaLink         String        @default("")
  welcomeHeader       String        @default("")
  welcomeDescription  String        @default("[{\"type\":\"paragraph\",\"children\":[{\"text\":\"\"}]}]")
  meetTeamHeader      String        @default("")
  findLocationHeader  String        @default("")
  findLocationEmbed   String        @default("")
  providers           Provider[]    @relation("Home_providers")
  servicesHeader      String        @default("")
  services            Service[]     @relation("Home_services")
  testimonialsHeader  String        @default("")
  testimonials        Testimonial[] @relation("Home_testimonials")
}

model About {
  id                  String  @id @default(cuid())
  title               String  @default("")
  heroImage_id        String?
  heroImage_filesize  Int?
  heroImage_width     Int?
  heroImage_height    Int?
  heroImage_extension String?
  content             String  @default("[{\"type\":\"paragraph\",\"children\":[{\"text\":\"\"}]}]")
}

model Location {
  id        String    @id @default(cuid())
  name      String    @default("")
  address   String    @default("")
  phone     String    @default("")
  fax       String    @default("")
  latitude  String    @default("")
  longitude String    @default("")
  services  Service[] @relation("Location_services")
}

model LocationLanding {
  id                 String @id @default(cuid())
  title              String @default("")
  subtitle           String @default("")
  findALocationLabel String @default("")
  mapEmbed           String @default("")
}

model Provider {
  id                  String  @id @default(cuid())
  name                String  @default("")
  email               String  @default("")
  title               String  @default("")
  image_id            String?
  image_filesize      Int?
  image_width         Int?
  image_height        Int?
  image_extension     String?
  type                String?
  posts               Post[]  @relation("Post_author")
  description         String  @default("[{\"type\":\"paragraph\",\"children\":[{\"text\":\"\"}]}]")
  homeDescription     String  @default("[{\"type\":\"paragraph\",\"children\":[{\"text\":\"\"}]}]")
  from_Home_providers Home[]  @relation("Home_providers")
}

model ProvidersLanding {
  id                  String @id @default(cuid())
  title               String @default("")
  dermatologistsLabel String @default("Dermatologists")
  npLabel             String @default("Nurse Practitioners")
}

model AppointmentsPage {
  id    String @id @default(cuid())
  title String @default("")
}

