  scheduleAnApptHeader    String  @default("")
  scheduleVisitLabel      String  @default("")
  scheduleVisitLink       String  @default("")
  contactUsLabel          String  @default("")
  phone                   String  @default("")
  fax                     String  @default("")
  instagramLink           String  @default("")
  linkedInLink            String  @default("")
  facebookLink            String  @default("")
}
